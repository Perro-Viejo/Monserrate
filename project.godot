"class": "GUI",
"language": "GDScript",
"path": "res://src/gui/GUI.gd"
}, {
"base": "Node2D",
"class": "Game",
"language": "GDScript",
"path": "res://src/Game.gd"
}, {
"base": "Node2D",
"class": "Home",
"language": "GDScript",
"path": "res://src/scenes/Home.gd"
} ]
_global_script_class_icons={
"GUI": "",
"Game": "",
"Home": ""
}

[application]

config/name="Monserrate"
config/description="Un juego sobra una estatua humana para el Stayhome GameJam, representa la informalidad laboral."
run/main_scene="res://src/Game.tscn"
config/icon="res://icon.png"

[autoload]

EventsManager="*res://src/managers/EventsManager.gd"

[display]

window/size/width=320
window/size/height=240

[rendering]

quality/driver/driver_name="GLES2"
vram_compression/import_etc=true
vram_compression/import_etc2=false
environment/default_environment="res://default_env.tres"
